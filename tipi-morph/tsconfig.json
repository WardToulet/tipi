{
  "compilerOptions": {
    "target": "es5",                                
    "module": "commonjs",                           
    "outDir": "dist",                               
    "rootDir": "src",                               
    "strict": true,                                 
    "esModuleInterop": true,                                    
    "skipLibCheck": true,                           
    "forceConsistentCasingInFileNames": true,
    "plugins": [
      {
        "transform": "./src/transform.ts",
        "type": "program"
      }
    ]
  }
}
